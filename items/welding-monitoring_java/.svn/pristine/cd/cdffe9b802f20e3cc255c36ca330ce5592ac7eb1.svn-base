server:
    port: 8072
spring:
    jackson:
        property-naming-strategy: SNAKE_CASE
        date-format: yyyy-MM-dd HH:mm:ss
        locale: zh
        time-zone: GMT+8
    application:
        name: welding-monitoring
    datasource:
        druid:
            #数据库地址
            username: root
            password: 123456
            url: jdbc:mysql://172.16.16.105/zj_weld_monitor?useUnicode=true&characterEncoding=UTF8&serverTimezone=Asia/Shanghai
            driver-class-name: com.mysql.cj.jdbc.Driver
    web:
        resources:
            static-locations: classpath:/static/
    mvc:
        pathmatch:
            matching-strategy: ant_path_matcher
    servlet:
        multipart:
            location: /static/resources/
            max-file-size: 50MB
            max-request-size: 50MB
    resources:
        static-locations: file:static/resources
#    filePath: C:/Users/45046/Desktop/welding-monitor/welding-monitoring_java/static/resources/
mybatis-plus:
